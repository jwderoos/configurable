{
  "name": "jwderoos/configurable",
  "description": "Package for self configuring services",
  "license": "proprietary",
  "type": "library",
  "require": {
      "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "phpstan/phpstan": "^2.1",
    "rector/rector": "^2.2",
    "sbrindle/phpcpd": "^7.0",
    "roave/security-advisories": "dev-latest",
    "phpstan/phpstan-beberlei-assert": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/extension-installer": "^1.4",
    "phpro/grumphp": "^2.17",
    "phpmd/phpmd": "^2.15",
    "enlightn/security-checker": "^2.0"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "phpro/grumphp": true
    }
  }
}
