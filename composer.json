{
  "name": "jwderoos/configurable",
  "description": "Package for self configuring services",
  "license": "proprietary",
  "type": "library",
  "require": {
      "php": "^8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "phpstan/phpstan": "^2.1",
    "rector/rector": "^2.2",
    "sbrindle/phpcpd": "^7.0",
    "roave/security-advisories": "dev-latest",
    "phpstan/phpstan-beberlei-assert": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/extension-installer": "^1.4",
    "phpro/grumphp": "^2.17",
    "phpmd/phpmd": "^2.15",
    "enlightn/security-checker": "^2.0",
    "squizlabs/php_codesniffer": "^4.0",
    "povils/phpmnd": "^3.6",
    "symfony/var-dumper": "^7.0",
    "infection/infection": "^0.31.9"
  },
  "autoload": {
    "psr-4": {
      "jwderoos\\Configurable\\tests\\": "tests/",
      "jwderoos\\Configurable\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "phpro/grumphp": true,
      "infection/extension-installer": true
    }
  }
}
